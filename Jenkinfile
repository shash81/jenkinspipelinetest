pipeline {
    agent any

    stages(){
        stage('INIT Stage'){
            steps{
                 echo  "this is INIT stage"
            }
        }
        stage('Build Stage'){
            steps{
                 echo  "this is Build stage"
            }
        }
       
      stage('Build and Test'){
        parallel(
        'build':{ stage('Build Stage'){
            steps{
                 echo  "this is INIT stage"
            }
        }
        },
        'test':{
        stage('Test Stage'){
            steps{
                 echo  "this is Build stage"
            }   
        
        
        }
        })
        }
        }

    }
}
