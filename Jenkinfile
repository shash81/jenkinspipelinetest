pipeline {
    agent any

    stages(){
        stage('INIT Stage'){
            steps{
                 echo  "this is INIT stage"
            }
        }
        stage('Build Stage'){
            steps{
                 echo  "this is Build stage"
            }
        }
        stage()
        parallel{
        stages{
        stage('Build Stage'){
            steps{
                 echo  "this is INIT stage"
            }
        }
        stage('Test Stage'){
            steps{
                 echo  "this is Build stage"
            }
        }
        
        }
        }
        }

    }
}
